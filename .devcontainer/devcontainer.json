{
    "name": "mstpylib",
    "dockerComposeFile": "../docker-compose.yaml",
    "service": "mstpylib",
    "context": "..",
    "workspaceFolder": "/local/mstpylib",
    "workspaceMount": "source=${localWorkspaceFolder},target=/local/mstpylib,type=bind",
    "overrideCommand": true,
    "runArgs": [
        "--name=mstpylib"
    ],
    "extensions": [
        "ms-python.python"
    ]
}